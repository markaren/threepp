@PACKAGE_INIT@
include ("@PACKAGE_targetsFile@")

include(CMakeFindDependencyMacro)

if (NOT DEFINED EMSCRIPTEN AND @THREEPP_WITH_GLFW@ AND @THREEPP_WITH_EXTERNAL_GLFW@)
    find_dependency(glfw3 CONFIG)

    if (NOT TARGET "glfw::glfw" AND TARGET "glfw")
        add_library(glfw::glfw ALIAS glfw)
    endif()
endif()

include(${CMAKE_CURRENT_LIST_DIR}/threepp-targets.cmake)
check_required_components(threepp)
